<SceneText>
  <Speakers>
    <Section>Speaker</Section>
    <Entry>
      <JapaneseText>スタン</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>フィリア</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ルーティ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <JapaneseText>渡す</JapaneseText>
      <EnglishText/>
      <Notes>Choice 1</Notes>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>保留にする</JapaneseText>
      <EnglishText/>
      <Notes>Choice 2</Notes>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>二人とも、今日はありがとう</JapaneseText>
      <EnglishText/>
      <Notes>Previous Choice 2 leads here</Notes>
      <SpeakerId>8</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>いえ、力になれてよかったです</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>11</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>どこに行ってたのよ、スタン</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>9</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ああ、ほら。
こいつを採りに行ってたんだ。
ルチルクォーツ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>１個は、ルチルブライトにして
将来の資金に使おう</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>もう１個は、プレゼントだ。
ルーティが好きなように使えばいいよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>スタンったら…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>9</SpeakerId>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>……………</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>11</SpeakerId>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>スタンさん、ルーティさんが
喜ぶだろうと一生懸命探していたんですよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>11</SpeakerId>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>それじゃ、私はこれで…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>11</SpeakerId>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>あ…、フィリアは
手に入れられなかったんだったな。
俺ばっかりはしゃいでごめん</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>んじゃ、今度はフィリアの分も
採りに行かなきゃ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>機会があれば、でいいですよ。
そのお気持ちだけで充分です</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>11</SpeakerId>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>じゃあ、今度</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>15</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>はい</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>11</SpeakerId>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
</SceneText>
