<SceneText>
  <Speakers>
    <Section>Speaker</Section>
    <Entry>
      <JapaneseText>ウッドロウ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>&lt;Name:82&gt;</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>130</Id>
      <Status>To Do</Status>
    </Entry>
  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <JapaneseText>付き合ってもらってすまないな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>12</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>この奥に、遣いの者がいる。
もう少し、ましな贈り物が
出来ればいいんだが</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>12</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>…？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>130</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>災害国に救援物資を届けたいんだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>12</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>けれども、今の私には
大した事は出来なくてね。
…少し情けないだけさ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>12</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>だが、動かねばな…。
動かねば何者も救えない</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>12</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
</SceneText>
