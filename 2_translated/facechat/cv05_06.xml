<SceneText>
  <Speakers>
    <Section>Speaker</Section>
    <Entry>
      <JapaneseText>ナナリー</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>カイル</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>19</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>リアラ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <JapaneseText>どうだい？
船の連中は親切にしてくれてるかい？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>21</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>うん、みんないい人だよ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>19</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ええ、親切だわ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>20</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ひょっとしてナナリー。
わたし達の事、良くしてくれるように
言ってくれた？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>20</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>まさか、そんな事しないよ。
あんたらがいい子だから、
みんな良くしてくれんのさ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>21</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ホントに？
嬉しいなぁ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>19</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ああ、だから、
ギルドのみんなに恩返ししないとね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>21</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ええ、頑張らないと</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>20</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ああ、そうしとくれ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>21</SpeakerId>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
</SceneText>
