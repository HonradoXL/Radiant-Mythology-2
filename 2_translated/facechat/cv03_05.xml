<SceneText>
  <Speakers>
    <Section>Speaker</Section>
    <Entry>
      <JapaneseText>スタン</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ルーティ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>リアラ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <JapaneseText>ふぁぁ〜〜〜〜、眠い…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ほら、
さっさと目を覚ましなさいな。
何が起こるかわかんないんだからさ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>9</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>そ、そうだよね。
みんなを守らなぁぁぁぁはあぅふ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>欠伸混じりで言わないでよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>9</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>………</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>20</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>あら、どーしたの？
じーっとあたしを見ちゃってさぁ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>9</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>いえ、仲がいいなぁって…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>20</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>そ〜ぉ？
こいつの天然に付き合うのって、
結構大変なのよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>9</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ええ？　俺って天然かなぁ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>で、天然って何？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>…ほらね？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>9</SpeakerId>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>カイルの上を行ってます</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>20</SpeakerId>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>…あれは遺伝だったのね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>20</SpeakerId>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
</SceneText>
