<SceneText>
  <Speakers>
    <Section>Speaker</Section>
    <Entry>
      <JapaneseText>コレット</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>25</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ティア</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>40</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>リアラ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ルーク</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>39</Id>
      <Status>To Do</Status>
    </Entry>
  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <JapaneseText>あ、ティアさん、ルークさん、
こんにちは</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>25</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>お話中？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>40</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>そうなの</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>20</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>………</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>40</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>どーしたんだ？　ティア</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>39</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ねえ、二人とも、年はいくつ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>40</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>１６だけど？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>25</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>わたしも…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>20</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>でええッ！！！
マジで？
ティアと同い年…？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>39</SpeakerId>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>…ウソでしょ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>20</SpeakerId>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>…本当よ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>40</SpeakerId>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>なんでおまえ、そんなに
育っちまったんだ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>39</SpeakerId>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>すごいねー。
私、全然大人っぽくなれないから
ティアさんが羨ましいな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>25</SpeakerId>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>いいなぁ…。
ティアぐらい大きかったら、
カイルは喜んでくれたかしら</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>20</SpeakerId>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>そんなに大きくなったら、
カイルの身長を追い越しちゃう。
今ぐらいがちょうどいいよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>25</SpeakerId>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>いえ、そっちじゃなくて…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>20</SpeakerId>
      <Id>15</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>し、身長の事は言ってやるなよ！
気にするからなっ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>39</SpeakerId>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>気にしてるのはルークでしょ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>40</SpeakerId>
      <Id>17</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>と、とにかく同い年なんだから、
私に「さん」付けはやめてもらえないかしら</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>40</SpeakerId>
      <Id>18</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>うん、わかった。
ティアさん</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>25</SpeakerId>
      <Id>19</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>…じゃないや。
ティア</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>25</SpeakerId>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>もういいんじゃない？
年上で</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>20</SpeakerId>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>だよね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>25</SpeakerId>
      <Id>22</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>よくないわよ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>40</SpeakerId>
      <Id>23</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
</SceneText>
