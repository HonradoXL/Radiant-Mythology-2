<SceneText>
  <Speakers>
    <Section>Speaker</Section>
    <Entry>
      <JapaneseText>スパーダ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>49</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>リオン</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ナナリー</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>アニス</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>43</Id>
      <Status>To Do</Status>
    </Entry>
  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <JapaneseText>…なーんか腹減らねェ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>49</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>減っていない。
食事の時間まで我慢するんだな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>10</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>あたしが何か作ってあげようか？
ちょうど手も空いてるし</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>21</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>マージで？
じゃ、頼もっかな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>49</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>って、アレ？
いい匂いがすんぜ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>49</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>じゃじゃ〜〜〜ん♪
と、ここでアニスちゃん登場！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>43</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>はい、スパーダ様〜☆
私のお手製の料理ですよ〜</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>43</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>オレ名指し…、
って事ぁオレの為？
うっそ、マジで？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>49</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>もっちろんですとも☆
自信作ですよー。
よかったら皆さんもどうぞ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>43</SpeakerId>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ふ〜ん、もらっとこうかね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>21</SpeakerId>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>…うん、アニスって見かけによらず
上手だね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>21</SpeakerId>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>………</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>10</SpeakerId>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>にんじんを除けてくれ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>10</SpeakerId>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>リオン、おまえェ、
腹減ってねェつってたろ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>49</SpeakerId>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>気が変わったんだ。
黙って食べてろ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>10</SpeakerId>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>意外だ</JapaneseText>
      <EnglishText/>
      <Notes>Choice 1</Notes>
      <Id>15</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>上手だ</JapaneseText>
      <EnglishText/>
      <Notes>Choice 2</Notes>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ふっふ〜ん、男を落とすには
まず胃袋からってね☆</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>43</SpeakerId>
      <Id>17</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>でっしょ〜？
私、頑張ったもんね</JapaneseText>
      <EnglishText/>
      <Notes>Previous Choice 2 leads here</Notes>
      <SpeakerId>43</SpeakerId>
      <Id>18</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>へへ、美味かったぜ。
アニス、サンキューなっ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>49</SpeakerId>
      <Id>19</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>まあまあだったな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>10</SpeakerId>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ケッ、なんだよその上から目線はよっ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>49</SpeakerId>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>僕は正直な感想を言っただけだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>10</SpeakerId>
      <Id>22</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ンだよ、あの野郎…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>49</SpeakerId>
      <Id>23</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>…今度は、スパーダ狙いって
わけかい？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>21</SpeakerId>
      <Id>24</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>どっきんっ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>43</SpeakerId>
      <Id>25</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>イリアから聞いたけど、
かなり家柄がいいみたいだもんね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>21</SpeakerId>
      <Id>26</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>あは、あはははは。
えっと、あ、お皿洗ってこよっと…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>43</SpeakerId>
      <Id>27</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>やれやれ、ホントちゃっかりしてるよ。
あの娘…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>21</SpeakerId>
      <Id>28</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
</SceneText>
