<SceneText>
  <Speakers>
    <Section>Speaker</Section>
    <Entry>
      <JapaneseText>リアラ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>リオン</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>リフィル</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>27</Id>
      <Status>To Do</Status>
    </Entry>
  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <JapaneseText>さあ、気をつけましょう。
ジュ…いえ、リオン</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>20</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>当たり前だ。
おまえに言われるまでもない</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>10</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>………</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>27</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>どうかしたか、リフィル</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>10</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>いえ、気になる事があってね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>27</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>それは、わたし達に関係する事？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>20</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ええ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>27</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>みんな名前に「リ」が付くなぁ…とね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>27</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>なんだ、そんな事か。
フン、つまらん</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>10</SpeakerId>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>あとは…、リッドとリリスさんかしら</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>20</SpeakerId>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>流石に全員一緒に
連れて来られないものね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>27</SpeakerId>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>おい、どうでもいい事を
しゃべっている暇があるなら、
とっとと先に進むぞ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>10</SpeakerId>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>そうね、そうしましょう</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>27</SpeakerId>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
</SceneText>
