    <Entry>
      <JapaneseText>ガイ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>41</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ナナリー</JapaneseText>
      <EnglishText/>
      <Notes/>
