<SceneText>
  <Speakers>
    <Section>Speaker</Section>
    <Entry>
      <JapaneseText>ナナリー</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>カイル</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>19</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>リオン</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <JapaneseText>何を拾って来たんだい？
カイル</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>21</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>…多分、なんかの動物の頭蓋骨だね。
随分大きいや。
頭に被れそうだもん</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>19</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>そんなものどうだっていいだろう。
とっとと捨てて来い</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>10</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>待ってよ、リオン。
…これ、リオンに似合うんじゃないかな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>19</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>な…、何を馬鹿な事を…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>10</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>あはは！　そりゃいいねぇ。
ねえ、ちょっと被ってごらんよ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>21</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>お断りだ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>10</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ねえ、いいじゃないか。
ちょっとだけだからさぁ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>19</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>断ると言っただろう</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>10</SpeakerId>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ほら！　ちょっと持ってみてよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>19</SpeakerId>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>………</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>10</SpeakerId>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>馬鹿馬鹿しい。
いい加減にしろ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>10</SpeakerId>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>付き合い悪いねぇ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>21</SpeakerId>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>………</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>10</SpeakerId>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>悪くない…かもな…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>10</SpeakerId>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>あれ？　リオン。
どうしたの？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>19</SpeakerId>
      <Id>15</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>な、なんでもないっ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>10</SpeakerId>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
</SceneText>
