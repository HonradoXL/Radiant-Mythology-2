<SceneText>
  <Speakers>
    <Section>Speaker</Section>
    <Entry>
      <JapaneseText>カイウス</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>45</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>カノンノ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>50</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ルビア</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>46</Id>
      <Status>To Do</Status>
    </Entry>
  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <JapaneseText>あーあー！
あ〜〜〜〜〜〜〜！！
えー、ホンジツハ　セイテン　ナリ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>45</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>…ど、どうしたの？
カイウス</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>50</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>嬉しそうだね</JapaneseText>
      <EnglishText/>
      <Notes>Choice 1</Notes>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>発声練習？</JapaneseText>
      <EnglishText/>
      <Notes>Choice 2</Notes>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>そりゃあ、嬉しいよ！
普通に声出せるなんて
気分最高だもんな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>45</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>そーなんだよ！
今まで、普通に喋る機会なんて
なかったからさ</JapaneseText>
      <EnglishText/>
      <Notes>Previous Choice 2 leads here</Notes>
      <SpeakerId>45</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>しっかり準備しとかないと
喉を傷めてしまうかもしれないもん</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>45</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>どういう意味…？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>50</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>どういう意味って、
そりゃ、なんていうか…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>45</SpeakerId>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>あ、ルビア！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>45</SpeakerId>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ら〜ら〜ら〜♪</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>46</SpeakerId>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>あら、カイウス！
声が出るって気持ちいいわねっ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>46</SpeakerId>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>だよな〜！
あはははははは！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>45</SpeakerId>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>えへへへへへへ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>46</SpeakerId>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>…一体なんだったの？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>50</SpeakerId>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
</SceneText>
