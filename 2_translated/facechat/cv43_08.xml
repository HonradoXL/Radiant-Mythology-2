    <Entry>
      <JapaneseText>クレア</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>33</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>イリア</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>48</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ルビア</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>46</Id>
      <Status>To Do</Status>
    </Entry>
  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <JapaneseText>○○！
ルビアを見なかったか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>45</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>見ていないと答える</JapaneseText>
      <EnglishText/>
      <Notes>Choice 1</Notes>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>首をひねる</JapaneseText>
      <EnglishText/>
      <Notes>Choice 2</Notes>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>くそ…、
どこ行ったんだよ！
あいつ…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>45</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>なんか迷惑そうな反応だな</JapaneseText>
      <EnglishText/>
      <Notes>Previous Choice 2 leads here</Notes>
      <SpeakerId>45</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>知らないんだったらいいさ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>45</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>どうかしたの？
慌ててるみたいだけど</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>33</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>トイレでも行きたいんじゃない？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>48</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>違うよ！
ルビアがいないんだよ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>45</SpeakerId>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>あいつまで…、いなくなって
しまったってのか…？
それとも連れ去られたのか…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>45</SpeakerId>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>うーん…。
じゃあ一度、
お手洗いに行ってみたらどうかな？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>33</SpeakerId>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>…クレアさん。
あんたまでそんな冗談言うのか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>45</SpeakerId>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>そうじゃなくて…。
その…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>33</SpeakerId>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ルビア、お腹の調子が
よくなかったみたいなの。
朝からこもりっきり</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>48</SpeakerId>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>そ、そうだったのか</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>45</SpeakerId>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>あ〜、スッキリ☆
クレアさん、
ハンカチありがとね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>46</SpeakerId>
      <Id>15</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ルビア！　おまえっ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>45</SpeakerId>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>なに？　どうしたの？
なんで怒ってんの？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>46</SpeakerId>
      <Id>17</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>そのっ！　なんていうか、
えーっと…だなっ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>45</SpeakerId>
      <Id>18</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>い…、胃腸は大事にしろよな！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>45</SpeakerId>
      <Id>19</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>…なんだったの、カイウスったら</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>46</SpeakerId>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>とりあえず、あなたの事を
心配してくれたみたいね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>33</SpeakerId>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>胃腸方面を主に、ねっ♪</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>48</SpeakerId>
      <Id>22</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>？？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>46</SpeakerId>
      <Id>23</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
</SceneText>
