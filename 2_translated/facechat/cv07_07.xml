<SceneText>
  <Speakers>
    <Section>Speaker</Section>
    <Entry>
      <JapaneseText>ガイ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>41</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ティア</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>40</Id>
      <Status>To Do</Status>
    </Entry>
  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <JapaneseText>…ルークの面倒を見させて悪いな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>41</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>突然、何？
今に始まった事じゃないわ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>40</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>いや、いつかちゃんと礼を
言っておかないとなって
思ってたのさ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>41</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ジェイドの旦那やアニスは
あまりルークのお守りに
真剣じゃないし</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>41</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>………</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>40</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>感謝してる。
本当に</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>41</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ガイ…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>40</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>おおっと！！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>41</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>…ごめんなさい</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>40</SpeakerId>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>我ながら難儀な体質だぜ…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>41</SpeakerId>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
</SceneText>
