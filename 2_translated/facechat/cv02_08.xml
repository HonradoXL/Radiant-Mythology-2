<SceneText>
  <Speakers>
    <Section>Speaker</Section>
    <Entry>
      <JapaneseText>カイル</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>19</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ルカ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>47</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>アニー</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>36</Id>
      <Status>To Do</Status>
    </Entry>
  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <JapaneseText>ルカもアニーもオレと同い年だよね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>19</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>あ、そういえばそうだった</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>47</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>１５歳同士…ですね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>36</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>でもなんでルカってそんなに
背が高いんだろ。
なんかズルいなぁ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>19</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>（カイルの背が低いんだと思うけど…）</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>47</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>イリアさんも同い年だけど、
背が高くて、スタイルもいいんです。
わたし、羨ましくって</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>36</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>馬に乗ってたら背が伸びたって、
イリアが言ってたけどね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>47</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>わたしも乗馬始めようかなぁ…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>36</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>オレも！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>19</SpeakerId>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
</SceneText>
