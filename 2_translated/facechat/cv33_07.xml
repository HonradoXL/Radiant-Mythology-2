<SceneText>
  <Speakers>
    <Section>Speaker</Section>
    <Entry>
      <JapaneseText>ルカ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>47</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>アニー</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>36</Id>
      <Status>To Do</Status>
    </Entry>
  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <JapaneseText>結構寒いね…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>47</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>他所から来た人には
そう感じるかもしれませんね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>36</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>厚着してきてよかった…。
イリアみたいにお腹出してたら
きっと風邪引いちゃう</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>47</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ええ、きっと引いちゃうでしょうね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>36</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>…な、何ですか？
何でわたしの方を見るんですか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>36</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>い、いや…。
アニーは平気なのかなって
思って…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>47</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>わたしはここが地元ですから。
慣れてます</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>36</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>慣れってすごいね…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>47</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
</SceneText>
