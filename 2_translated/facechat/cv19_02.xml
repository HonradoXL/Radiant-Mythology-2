    <Entry>
      <JapaneseText>リッド</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>15</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>キール</JapaneseText>
      <EnglishText/>
