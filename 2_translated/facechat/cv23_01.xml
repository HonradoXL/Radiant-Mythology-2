<SceneText>
  <Speakers>
    <Section>Speaker</Section>
    <Entry>
      <JapaneseText>アーチェ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>セネル</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>37</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>すず</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ファラ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <JapaneseText>ね〜、セネル！
クロエとあんたってば、
どういう関係なのよっ☆</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>…関係、だと？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>37</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>私の調べでは…、
それなりに親しい関係ではあるようです</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>6</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>親しい、って？
ねえねえ、それってどういう感じの？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>16</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>オレとクロエは
疑われるような関係じゃない</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>37</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>…疑われるとは？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>6</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>あ、いや…、違う！
そういう意味じゃない</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>37</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>じゃあクロエの事嫌いなの？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>16</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>い、いや…、嫌ってなんかない</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>37</SpeakerId>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>聞いた〜？
嫌いじゃないんだって！
ってことは〜</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>好きなんだ！
キャー！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>16</SpeakerId>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>それは素晴らしい事ですね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>6</SpeakerId>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>…付き合ってられないな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>37</SpeakerId>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>そんでさぁ！
もちろん、ただお話するだけの
間柄じゃないよね！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>かもしれません</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>6</SpeakerId>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>もちろんだよ！
きっと、もう、あんな事とかもう…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>15</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
</SceneText>
